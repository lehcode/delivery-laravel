{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alexpechkarev/google-geocoder": "dev-master",
        "barryvdh/laravel-cors": "^0.9.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "elasticsearch/elasticsearch": "^5.2",
        "graham-campbell/flysystem": "^3.7",
        "jenssegers/date": "^3.2",
        "laravel/dusk": "^1.1",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.14.0",
        "owen-it/laravel-auditing": "^4.0",
        "phaza/laravel-postgis": "^3.1",
        "predis/predis": "^1.1",
        "propaganistas/laravel-intl": "^1.2",
        "propaganistas/laravel-phone": "^2.8",
        "rap2hpoutre/laravel-credit-card-validator": "^0.0.6",
        "santigarcor/laratrust": "3.2.*",
        "spatie/image": "^1.2",
        "spatie/laravel-fractal": "^3.5",
        "spatie/laravel-medialibrary": "^5.0.0",
        "spatie/laravel-translatable": "^1.2",
        "squizlabs/php_codesniffer": "^2.8",
        "symfony/psr-http-message-bridge": "^1.0",
        "tymon/jwt-auth": "0.5.*",
        "watson/validating": "^3.0",
        "webpatser/laravel-uuid": "^2.0",
        "zendframework/zend-diactoros": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "doctrine/dbal": "~2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
